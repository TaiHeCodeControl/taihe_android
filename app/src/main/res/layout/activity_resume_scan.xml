<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_gray"
    android:orientation="vertical">

    <include
        android:id="@+id/findpass_title"
        layout="@layout/include_title_bar" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/line_gray"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <TextView
            android:id="@+id/id_resume_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="个人简历"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:layout_centerVertical="true"/>


        <TextView
            android:id="@+id/id_create_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:text="创建于2015-09-09"
            android:textSize="16sp"
            android:textColor="@color/font_color_gray"/>

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/line_gray"/>

    <ScrollView
        android:id="@+id/id_scan_scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingLeft="15dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/id_user_name"
                        android:text="王五"
                        style="@style/ResumeScanPeo"/>

                    <TextView
                        android:id="@+id/id_c"
                        android:layout_marginLeft="10dp"
                        android:text="("
                        style="@style/ResumeScanPeo" />

                    <TextView
                        android:id="@+id/id_gender"
                        android:layout_marginLeft="2dp"
                        android:text="男"
                        style="@style/ResumeScanPeo" />
                    <TextView
                        android:id="@+id/id_d"
                        android:layout_marginLeft="1dp"
                        android:text=","
                        style="@style/ResumeScanPeo"/>

                    <TextView
                        android:id="@+id/id_age"
                        android:layout_marginLeft="2dp"
                        android:text="100岁"
                        style="@style/ResumeScanPeo"/>

                    <TextView
                        android:id="@+id/id_e"
                        android:layout_marginLeft="1dp"
                        android:text=")"
                        style="@style/ResumeScanPeo" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="学历:\u3000\u3000"/>

                    <TextView
                        android:id="@+id/id_level"
                        style="@style/ResumeTextStyle"
                        android:text=""/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="经验:\u3000\u3000"/>

                    <TextView
                        android:id="@+id/id_experince"
                        style="@style/ResumeTextStyle"
                        android:text=""/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="所在地:\u3000"/>

                    <TextView
                        android:id="@+id/id_address"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="联系手机:" />

                    <TextView
                        android:id="@+id/id_telphone"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="电子邮件:" />

                    <TextView
                        android:id="@+id/id_email"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="求职意向"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_job_intention"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="编辑"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/edit_icon"/>
                    </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="期望职位:" />

                    <TextView
                        android:id="@+id/id_hope_position"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="期望行业:" />

                    <TextView
                        android:id="@+id/id_hope_industy"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="待遇要求:" />

                    <TextView
                        android:id="@+id/id_want_money"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="工作地区:" />

                    <TextView
                        android:id="@+id/id_work_ear"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="到职时间:" />

                    <TextView
                        android:id="@+id/id_come_time"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="求职状态:" />

                    <TextView
                        android:id="@+id/id_status"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="left|center"
                    android:paddingLeft="15dp"
                    android:orientation="horizontal">
                    <TextView
                        style="@style/ResumeTextStyle_left"
                        android:text="职位性质:" />

                    <TextView
                        android:id="@+id/id_position_type"
                        style="@style/ResumeTextStyle"
                        android:text="" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_work_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="工作经历"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_work"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_work_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/line_gray"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_edu_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="教育经历"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_edu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_edu_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/white"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_skill_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="专业技能"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_tech"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_tech_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/white"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_project_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="项目经验"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_project"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_project_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/white"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_book_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="证书"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_book"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_book_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/white"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_train_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="培训经历"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_train"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="新增"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <com.taihe.eggshell.widget.MyListView
                    android:id="@+id/id_train_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@color/white"
                    android:dividerHeight="0.5dp"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/id_self_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="10dp"
                android:paddingBottom="20dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:background="@color/scan_button_color">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="自我评价"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"/>

                    <TextView
                        android:id="@+id/id_to_add_self"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="编辑"
                        android:textSize="@dimen/font_size_15"
                        android:textColor="@color/white"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/add_icon"/>
                </RelativeLayout>

                <TextView
                    android:id="@+id/id_self_desc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="8dp"
                    android:paddingLeft="15dp"
                    android:text=""
                    android:textColor="@color/black"/>

            </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </LinearLayout>